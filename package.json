{
  "name": "moka2popsy",
  "version": "0.0.1",
  "description": "Transformation d'un fichier Moka en Popsy",
  "main": "main.js",
  "scripts": {
    "start": "electron app/main.js",
    "test": "xo",
    "build": "electron-packager app/main.js --out=dist --asar --overwrite --all",
    "dist": "build app/main.js -"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Proglab/electron.git"
  },
  "keywords": [],
  "author": "Fabrice Gyre",
  "license": "private",
  "devDependencies": {
    "asar": "^0.13.0",
    "devtron": "^1.4.0",
    "electron-builder": "^19.24.1",
    "electron-debug": "^1.4.0",
    "electron-packager": "^8.7.2",
    "xo": "^0.18.2",
    "electron": "^1.7.5"
  },
  "build": {
    "appId": "com.proglab.moka2popsy",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {},
  "xo": {
    "envs": [
      "node",
      "browser"
    ]
  }
}
